<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="600" d:DesignHeight="200"
        x:Class="курчас.clientAdapterSec"
        Title="clientAdapterSec">
	<DockPanel Background="#D8BFD8">
		<StackPanel>
			<Button
					   Click="ClientAdapterSecBack_OnClick"
					   Name="ClientAdapterSecBack"
					   Content="Выход"
					   FontSize="15"
					   Width="65" Height="30"
					   CornerRadius="45"
					   VerticalContentAlignment="Bottom"
					   HorizontalContentAlignment="Left"
					   Margin="10, 10"/>
		</StackPanel>
		<StackPanel Orientation="Vertical"
					VerticalAlignment="Center"
					HorizontalAlignment="Center"
					>
			<Button
	  Click="Create_Order_OnClick"
	  Name="Create_Order"
	  Content="Создать новый заказ"
	  FontSize="24"
	  Width="402" Height="56"
	  CornerRadius="45"
	  VerticalContentAlignment="Center"
	  HorizontalContentAlignment="Center"
	  Margin="15"/>
			<Button
					   Click="Check_History_OnClick"
					   Name="Check_History"
					   Content="Просмотреть историю заказов"
					   FontSize="24"
					   Width="402" Height="56"
					   CornerRadius="45"
					   VerticalContentAlignment="Center"
					   HorizontalContentAlignment="Center"
					   Margin="15, 0"/>
		</StackPanel>
	</DockPanel>
</Window>
